%param oldfont newfont
/ReEncodeFont {
  /new exch def
  /old exch def

  old findfont
  dup length dict begin
    {
      1 index /FID ne {def} {pop pop} ifelse} forall
    /Encoding ISOLatin1Encoding def
    currentdict
  end
  new exch definefont pop
} def

/Helvetica	/Helvetica	ReEncodeFont
/Helvetica-Bold /Helvetica-Bold	ReEncodeFont
/Courier	/Courier	ReEncodeFont
/Courier-Bold	/Courier-Bold	ReEncodeFont
/Times-Roman	/Times		ReEncodeFont
/Times-Bold	/Times-Bold	ReEncodeFont

